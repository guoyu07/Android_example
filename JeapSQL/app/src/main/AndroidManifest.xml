        </activity>

        <meta-data android:name="AA_DB_NAME" android:value="Weather.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="5" />

        <meta-data
            android:name="AA_MODELS"
            android:value="com.jeap.asmcos.jeapsql.CityInfo" />
    </application>

</manifest>
